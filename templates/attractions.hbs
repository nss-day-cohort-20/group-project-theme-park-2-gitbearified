

<div class="attraction">
    {{#each this}}
        <h3 class="linkLike">{{name}}</h3>
        <h4>({{type}})</h4>
        <p class="isHidden">{{description}}</p>
        {{#if times}}
        	<p class="isHidden">{{times}}</p>
        {{else}}
        	<p class="isHidden">{{hours}}</p>
        {{/if}}
    {{/each}}
</div>